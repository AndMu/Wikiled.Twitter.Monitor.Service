{
  "Logging": {
    "path": "${basedir}/logs",
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "sentiment": {
    "track":  true, 
    "url": "http://192.168.0.70:7017",
    "domain": "TwitterMarket"
  },
  "twitter": {
    "persistency": "/logging/TwitterMarket/data",
    "hashkeywords":  false, 
    "users": [  ],
    "keywords": [ "$AMD", "$GOOG", "$FB", "$MMM", "$CAT", "$AMZN", "$AXP", "$KO", "$INTC", "$PM", "$TIF", "$WFC", "$MS", "$JPM" ],
    "languages": [ "English" ]
  }
}
