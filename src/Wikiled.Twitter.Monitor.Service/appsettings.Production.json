{
  "Logging": {
    "path": "/logging/_location_",
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "sentiment": {
    "track":  _track_, 
    "url": "_sentiment.url_",
    "domain": "_domain_"
  },
  "twitter": {
    "persistency": "/logging/_location_/data",
    "hashkeywords":  _hashkey_, 
    "users": [ _users_ ],
    "keywords": [ _keywords_ ],
    "languages": [ _languages_ ]
  }
}
