{
  "Logging": {
    "path": "c:/logging/$location$",
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information"
    },
    "sentiment": {
      "url": "http://",
      "domain": "$domain$"
    },
    "twitter": {
      "persistency": "c:/logging/$location$/data",
      "keywords": [ "$keywords$" ],
      "languages": [ "$languages$" ]
    }
  }
}
