{
  "Logging": {
    "path": "c:/logging/$location$",
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "sentiment": {
    "url": "$sentiment.$url$",
    "domain": "$domain$"
  },
  "twitter": {
    "persistency": "c:/logging/$location$/data",
    "keywords": [ "$keywords$" ],
    "languages": [ "$languages$" ]
  }
}
